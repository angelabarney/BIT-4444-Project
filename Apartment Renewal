    <script type="text/javascript">

    function checkForm(event) {
      var frmObject = document.forms[0];
      badStr="You must fill in a value for:\n";
      notFinished=false;

      //validate radio buttons
      if(isUncheckedRB(frmObject.membership)) notFinished=true;
      if(isUncheckedRB(frmObject.payment)) notFinished = true;
      if(isUncheckedRB(frmObject.renew)){
        notFinished=true;
      } else {
        //validate text box
        if(isBlankField(frmObject.nonrenew)) notFinished=true;
      }}
      console.log(notFinished);
      if(notFinished) {
        alert(badStr);
        event.preventDefault();
        return false;
      }
      else {
        alert("Input validated");
        return true;
      }
    }
    
    </script>
